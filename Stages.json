{
  "table": "Stages",
  "columns": [
    {
      "name": "Id",
      "type": "INT",
      "identity": {
        "seed": 1,
        "increment": 1
      },
      "not_null": true,
      "primary_key": true
    },
    {
      "name": "StageName",
      "type": "NVARCHAR(255)",
      "not_null": true
    },
    {
      "name": "StageWaveCount",
      "type": "INT",
      "not_null": true,
      "default": 0
    },
    {
      "name": "StageUnlocked",
      "type": "INT",
      "not_null": true,
      "default": 0,
      "comment": "0: Locked, 1: Unlocked"
    }
  ]
}